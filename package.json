{
  "name": "@immowelt/babel-preset-immowelt-react",
  "version": "0.0.0-development",
  "private": false,
  "author": "Immonet dev team <aegaeon@immonet.de> (https://immonet.de)",
  "license": "UNLICENSED",
  "description": "The babel preset for an react environment",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run lint && npm run jest:coverage",
    "lint": "eslint src",
    "jest": "jest",
    "jest:watch": "npm run jest -- --watch",
    "jest:coverage": "npm run jest -- --coverage",
    "semantic-release": "semantic-release pre && npm publish --access=public && semantic-release post"
  },
  "dependencies": {
    "babel-runtime": "6.26.0",
    "babel-preset-react-app": "^3.0.1"
  },
  "devDependencies": {
    "@inkdpixels/commit-analyzer": "^1.0.0",
    "@inkdpixels/release-notes-generator": "^1.0.0",
    "@immowelt/eslint-config-immowelt-react": "^1.0.0",
    "eslint": "4.5.0",
    "jest": "^20.0.4",
    "semantic-release": "^7.0.1"
  },
  "engines": {
    "node": ">=8.2.0",
    "npm": ">=5.3.0"
  },
  "release": {
    "analyzeCommits": "@inkdpixels/commit-analyzer",
    "generateNotes": "@inkdpixels/release-notes-generator"
  },
  "browserslist": [
    "last 1 versions"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "testMatch": [
      "<rootDir>/src/**/?(*.)spec.js"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.js$"
    ],
    "moduleFileExtensions": [
      "js"
    ]
  },
  "keywords": [
    "babel",
    "immowelt",
    "preset",
    "immonet"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ImmoweltHH/babel-preset-immowelt-react.git"
  }
}
