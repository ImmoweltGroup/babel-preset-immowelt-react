{
  "name": "@immowelt/babel-preset-immowelt-react",
  "version": "0.0.0-development",
  "private": false,
  "author": "Immonet dev team <aegaeon@immonet.de> (https://immonet.de)",
  "license": "MIT",
  "description": "The babel preset for an react environment",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run lint && npm run jest:coverage",
    "lint": "eslint src",
    "prettier": "prettier --write src/**/*.js",
    "jest": "jest",
    "jest:watch": "npm run jest -- --watch",
    "jest:coverage": "npm run jest -- --coverage",
    "semantic-release": "semantic-release pre && npm publish --access=public && semantic-release post",
    "precommit": "lint-staged && npm test"
  },
  "lint-staged": {
    "src/**/*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "babel-plugin-flow-react-proptypes": "^9.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "6.26.0"
  },
  "devDependencies": {
    "@immowelt/eslint-config-immowelt-react": "^2.0.0",
    "@immowelt/jest-preset-node": "^1.0.0",
    "@immowelt/styleguide-javascript": "^1.0.6",
    "@inkdpixels/commit-analyzer": "^1.0.0",
    "@inkdpixels/release-notes-generator": "^1.0.0",
    "eslint": "4.10.0",
    "babel-core": "^6.26.0",
    "husky": "^0.14.3",
    "jest": "^21.0.2",
    "lint-staged": "^4.1.2",
    "prettier": "^1.6.1",
    "semantic-release": "^8.0.0"
  },
  "engines": {
    "node": ">=8.2.0",
    "npm": ">=5.3.0"
  },
  "release": {
    "analyzeCommits": "@inkdpixels/commit-analyzer",
    "generateNotes": "@inkdpixels/release-notes-generator"
  },
  "jest": {
    "preset": "@immowelt/jest-preset-node"
  },
  "keywords": [
    "babel",
    "immowelt",
    "preset",
    "immonet"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ImmoweltGroup/babel-preset-immowelt-react.git"
  }
}
